{
  "mcpServers": {
    "ai-therapy-memory": {
      "command": "/path/to/your/python",
      "args": [
        "/path/to/mcp-ai-therapy/mcp-server/src/main.py"
      ],
      "env": {
        "MEMORY_DATA_DIR": "/path/to/mcp-ai-therapy/memory_data",
        "OPENAI_API_KEY": "your-openai-api-key-here",
        "LOG_LEVEL": "INFO"
      }
    }
  }
}
